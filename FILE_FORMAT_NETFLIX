CREATE OR REPLACE FILE FORMAT netflix_csv
TYPE = CSV
FIELD_OPTIONALLY_ENCLOSED_BY = '"'
SKIP_HEADER = 1;

CREATE OR REPLACE STAGE netflix_stage;

COPY INTO NETFLIX.PUBLIC.NETFLIX
FROM @netflix_stage/netflix_titles.csv
FILE_FORMAT = (
    TYPE = CSV
    FIELD_OPTIONALLY_ENCLOSED_BY = '"'
    SKIP_HEADER = 1
);
